{
    "name": "react-ts-tailwind-starter",
    "version": "1.0.0",
    "description": "React 19, TypeScript, Tailwind CSS, and Webpack 6 starter template.",
    "keywords": [
        "react 19",
        "typescript",
        "tailwindcss",
        "webpack 6",
        "starter"
    ],
    "license": "MIT",
    "type": "module",
    "author": {
        "name": "Ignacio Villamar",
        "email": "iggy@ivstudio.com",
        "url": "https://github.com/ivstudio"
    },
    "homepage": "https://mum37352.github.io/benes",
    "scripts": {
        "start": "cross-env NODE_OPTIONS='--loader=ts-node/esm' webpack serve --config webpack/webpack.dev.ts",
        "start-prod": "cross-env NODE_OPTIONS='--loader=ts-node/esm' webpack serve --config webpack/webpack.prod.ts",
        "test": "jest",
        "analyze": "ANALYZE=true npm run build",
        "build": "cross-env NODE_OPTIONS='--loader=ts-node/esm' npx webpack --config webpack/webpack.prod.ts",
        "coverage": "jest --coverage",
        "format": "prettier --write src",
        "lint": "eslint ./src",
        "lint:debug": "eslint ./src --debug",
        "lint:fix": "eslint ./src --fix",
        "prepare": "husky"
    },
